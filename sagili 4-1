#include <stdio.h>
int main() 
{
    int m,n;
    printf("Enter the starting point:");
    scanf("%d",&m);
    printf("Enter the ending point:");
    scanf("%d",&n);
    printf("the even number in the given range are \n:");
    for(int i=m;i<=n;i++)
    {
        if(i%2==0);{
        printf("%d ",i);}
}
printf("the odd number in the given range are \n:");
for (int i=m;i<=n;i++){
    if (i%2!=0){
        printf("%d",i);
    }
}
    

    return 0;
    }
